      "startupFunc": "main",
      "startupArgs": "",
      "stopOnEntry": true,
      "mainArgs": "",
      "ghciPrompt": "Debug>",
      "ghciInitialPrompt": "Prelude>",
      "ghciCmd": "stack ghci --main-is services:exe:access-control",
      "ghciEnv": {},
      "logFile": "${workspaceRoot}/.vscode/phoityne.log",
      "logLevel": "WARNING"
    }
  ]
}