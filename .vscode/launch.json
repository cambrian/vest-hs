{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "ghc",
      "request": "launch",
      "name": "tezos-chain-watcher",
      "internalConsoleOptions": "openOnSessionStart",
      "workspace": "${workspaceRoot}",
      "startup": "${workspaceRoot}/services/tezos-chain-watcher/bin/Main.hs",
      "startupFunc": "main",
      "startupArgs": "",
      "stopOnEntry": true,
      "mainArgs": "",
      "ghciPrompt": "Debug>",
      "ghciInitialPrompt": "Prelude>",
      "ghciCmd": "stack ghci --main-is services:exe:tezos-chain-watcher",
      "ghciEnv": {},
      "logFile": "${workspaceRoot}/.vscode/phoityne.log",
      "logLevel": "WARNING"
    },
    {
      "type": "ghc",
      "request": "launch",
